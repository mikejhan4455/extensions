{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "call",
  "title": "Call",
  "description": "Simply making a phone call.",
  "icon": "command-icon.png",
  "author": "mikejhan4455",
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "call",
      "title": "Call",
      "description": "Phone number",
      "mode": "no-view",
      "arguments": [
        {
          "name": "number",
          "placeholder": "number",
          "type": "text",
          "required": true
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.53.4"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}